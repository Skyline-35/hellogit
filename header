#include <iostream>
#include <string>
using namespace std;

class Car {
private:
	int wheels;
	string chassisNum= " ";

public:
	Car(){
		wheels= 0;
		chassisNum= " ";
	     }
	
	void setchassisNum(string c){

		chassisNum=c;
	     }

	string getchassisNum(){

		return chassisNum;
	     }
	void setWheels (string w){

		wheels=w;
	     }
	int getWheels(){
	     
		return wheels;
	     }

